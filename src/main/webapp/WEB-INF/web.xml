<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd" id="WebApp_ID" version="6.0">
  <display-name>my_web-app</display-name>
  <welcome-file-list>
    <welcome-file>home.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.html</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>exampleServlet</display-name>
    <servlet-name>exampleServlet</servlet-name>
    <servlet-class>com.servlets.exampleServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>exampleServlet</servlet-name>
    <url-pattern>/example</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>myGenericServlet</display-name>
    <servlet-name>myGenericServlet</servlet-name>
    <servlet-class>com.servlets.myGenericServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>myGenericServlet</servlet-name>
    <url-pattern>/myGenericServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>form</display-name>
    <servlet-name>form</servlet-name>
    <servlet-class>com.servlets.form</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>form</servlet-name>
    <url-pattern>/form</url-pattern>
  </servlet-mapping>
  <servlet>
    <description>Users use this servlet to register</description>
    <display-name>Register</display-name>
    <servlet-name>Register</servlet-name>
    <servlet-class>com.servlets.Register</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Register</servlet-name>
    <url-pattern>/Register</url-pattern>
  </servlet-mapping>
  <servlet>
    <description>home page of registered user</description>
    <display-name>userHome</display-name>
    <servlet-name>userHome</servlet-name>
    <servlet-class>com.servlets.userHome</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>userHome</servlet-name>
    <url-pattern>/userHome</url-pattern>
  </servlet-mapping>
</web-app>